<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.moddingcreator.controllers.menu.LoadModController"
            prefHeight="400.0" prefWidth="600.0">
    <Label fx:id="loadLabel" layoutX="150" layoutY="290">Saved Mods</Label>
    <ComboBox fx:id="savedMods" layoutX="140" layoutY="320"/>
    <Button fx:id="loadModButton" layoutX="300" layoutY="250" onAction="#onLoadModClick">Load Mod</Button>
</AnchorPane>
