<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.moddingcreator.controllers.mod.ModMenuController"
            prefHeight="400.0" prefWidth="600.0">
    <Label fx:id="modNameLabel" layoutX="400" layoutY="100"/>
    <Label fx:id="modIdLabel" layoutX="400" layoutY="250"/>

    <Button fx:id="mainMenuButton" layoutX="10" layoutY="570" onAction="#mainMenuClicked">Main Menu</Button>
    <Button fx:id="launchModButton" layoutX="400" layoutY="500" onAction="#launchModClicked">Launch Mod</Button>
</AnchorPane>
